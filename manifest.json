{
  "manifest_version": 3,
  "name": "AI & ML News",
  "version": "1.1.0",
  "description": "A Chrome extension that displays the latest AI and Machine Learning news",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "AI & ML News",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [],
  "host_permissions": [
    "https://newsapi.org/*"
  ]
}
